<scroll-view class="{{((('data-v-7b52bbac')+' '+'gui-scroll-x')+' '+(isCenter?'gui-nav-center':''))}}" style="{{'width:'+(width+'rpx')+';'}}" scroll-with-animation="{{scorllAnimation}}" scroll-x="{{true}}" show-scrollbar="{{false}}" scroll-left="{{scrollLeft}}"><block tt:for="{{itemsIn}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="gui-scroll-x-items gui-columns data-v-7b52bbac" style="{{'width:'+(size==0?'auto':size+'rpx')+';'+('margin-right:'+(margin+'rpx')+';')+('padding-left:'+(padding)+';')+('padding-right:'+(padding)+';')}}" id="{{'tab-'+index+(random+'')}}" data-index="{{index}}" data-event-opts="{{[['tap',[['change',['$event']]]]]}}" bindtap="__e"><view class="{{((((('gui-flex')+' '+'gui-nowrap')+' '+'gui-align-items-start')+' '+'data-v-7b52bbac')+' '+(textAlign=='center'?'gui-justify-content-center':''))}}"><text class="{{(((('data-v-7b52bbac')+' '+'gui-block-text')+' '+'gui-border-box')+' '+(currentIndexIn==index?'nav-active':''))}}" style="{{'color:'+(currentIndexIn==index?activeColor:color)+';'+('text-align:'+(textAlign)+';')+('line-height:'+(lineHeight)+';')+('font-size:'+(currentIndexIn==index?activeFontSize:fontSize)+';')+('font-weight:'+(currentIndexIn==index?activeFontWeight:'')+';')}}">{{item.name}}</text><block tt:if="{{item.tips}}"><view class="data-v-7b52bbac"><block tt:if="{{item.tips!='point'}}"><text class="gui-nav-tips gui-block-text data-v-7b52bbac" style="{{(tipsStyle)}}">{{item.tips}}</text></block><block tt:else><text class="gui-nav-tips gui-block-text data-v-7b52bbac" style="{{(tipsStyle+'; width:12rpx; height:12rpx; over-flow:hidden; padding:0rpx; margin-top:10rpx;')}}"></text></block></view></block></view><view class="gui-flex gui-rows data-v-7b52bbac" style="{{'justify-content:'+(activeDirection)+';'}}"><block tt:if="{{currentIndexIn==index}}"><view class="{{((('nav-active-line')+' '+'data-v-7b52bbac')+' '+(currentIndexIn==index&&animatie?'gui-nav-scale':''))}}" style="{{'background-image:'+(activeLineBg)+';'+('width:'+(activeLineWidth)+';')+('height:'+(activeLineHeight)+';')+('border-radius:'+(activeLineRadius)+';')}}"></view></block></view></view></block></scroll-view>