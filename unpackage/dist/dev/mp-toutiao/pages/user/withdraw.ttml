<view class="content gui-margin"><view class="row b-b"><text class="tit">支付宝账号</text><input class="input" type="text" placeholder="请输入支付宝账号" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','alipayCount','$event',[]],['data']]]]]}}" value="{{data.alipayCount}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">真实姓名</text><input class="input" type="text" placeholder="请输入真实姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','realname','$event',[]],['data']]]]]}}" value="{{data.realname}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">提现金额</text><input class="input" type="number" placeholder="请输入提现金额" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','value','$event',[]],['data']]]]]}}" value="{{data.value}}" bindinput="__e"/></view><block tt:if="{{tip}}"><view class="row b-b"><text class="tip">{{tip}}</text></view></block><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>