.demo {
  width: 210rpx;
  margin: 10rpx;
}
.button {
  width: 90rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 8rpx;
  margin: 10rpx;
}
.button-text {
  font-size: 38rpx;
  text-align: center;
}
/* 列表 */
.guess-section {
  display: flex;
  flex-wrap: wrap;
  padding: 0 30rpx;
  background: #fff;
}
.guess-section .guess-item {
  display: flex;
  flex-direction: column;
  width: 48%;
  padding-bottom: 40rpx;
}
.guess-section .guess-item:nth-child(2n+1) {
  margin-right: 4%;
}
.guess-section .image-wrapper {
  width: 100%;
  height: 330rpx;
  border-radius: 3px;
  overflow: hidden;
}
.guess-section .image-wrapper image {
  width: 100%;
  height: 100%;
  opacity: 1;
}
.guess-section .title {
  font-size: 24rpx;
  color: #606266;
  line-height: 80rpx;
}
.guess-section .price {
  font-size: 24rpx;
  color: #fa436a;
  line-height: 1;
}
.guess-section .price-box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right: 10rpx;
  font-size: 22rpx;
  color: #909399;
}
.guess-section .price {
  font-size: 32rpx;
  color: #fa436a;
  line-height: 1;
}

