page, .content {
  background: #f8f8f8;
}
.content {
  padding-top: 96rpx;
}
.navbar {
  position: fixed;
  left: 0;
  top: 0px;
  display: flex;
  width: 100%;
  height: 80rpx;
  background: #fff;
  box-shadow: 0 2rpx 10rpx rgba(0, 0, 0, 0.06);
  z-index: 10;
}
.navbar .nav-item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  font-size: 30rpx;
  color: #303133;
  position: relative;
}
.navbar .nav-item.current {
  color: #fa436a;
}
.navbar .nav-item.current:after {
  content: '';
  position: absolute;
  left: 50%;
  bottom: 0;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  width: 120rpx;
  height: 0;
  border-bottom: 4rpx solid #fa436a;
}
.navbar .p-box {
  display: flex;
  flex-direction: column;
}
.navbar .p-box .yticon {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 30rpx;
  height: 14rpx;
  line-height: 1;
  margin-left: 4rpx;
  font-size: 26rpx;
  color: #888;
}
.navbar .p-box .yticon.active {
  color: #fa436a;
}
.navbar .p-box .xia {
  -webkit-transform: scaleY(-1);
          transform: scaleY(-1);
}
.navbar .cate-item {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  width: 80rpx;
  position: relative;
  font-size: 44rpx;
}
.navbar .cate-item:after {
  content: '';
  position: absolute;
  left: 0;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  border-left: 1px solid #ddd;
  width: 0;
  height: 36rpx;
}
.dolls-list {
  display: flex;
  flex-wrap: wrap;
  padding: 0 30rpx;
  background: #fff;
}
.dolls-list .dolls-item {
  display: flex;
  flex-direction: column;
  width: 48%;
  padding-bottom: 40rpx;
}
.dolls-list .dolls-item:nth-child(2n+1) {
  margin-right: 4%;
}
.dolls-list .image-wrapper {
  width: 100%;
  height: 330rpx;
  border-radius: 3px;
  overflow: hidden;
}
.dolls-list .image-wrapper image {
  width: 100%;
  height: 100%;
  opacity: 1;
}
.dolls-list .title {
  font-size: 32rpx;
  color: #303133;
  line-height: 80rpx;
}
.dolls-list .price-box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right: 10rpx;
  font-size: 24rpx;
  color: #909399;
}
.dolls-list .price {
  font-size: 32rpx;
  color: #fa436a;
  line-height: 1;
}
.gui-shoppingcard {
  border-radius: 8rpx;
  padding: 20rpx 30rpx;
}
.gui-shopp-name {
  width: 200rpx;
  flex: 1;
}
.gui-shopp-go {
  width: 120rpx;
  text-align: right;
}
.gui-shoppingcard-goods {
  padding: 20rpx 0;
  height: 180rpx;
  width: 700rpx;
}
.gui-shoppingcard-goods-body {
  flex: 1;
  overflow: hidden;
}
.gui-shoppingcard-goods-status {
  color: #008AFF;
  font-size: 28rpx;
}
.gui-shoppingcard-goods-number {
  padding: 2px 0;
  font-size: 28rpx;
}

