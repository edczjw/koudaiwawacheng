<view class="content gui-margin"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','realname','$event',[]],['addressData']]]]]}}" value="{{addressData.realname}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['addressData']]]]]}}" value="{{addressData.phone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">所在地区</text><text data-event-opts="{{[['tap',[['openAddressPicker',['$event']]]]]}}" class="input" style="{{'color:'+(addressData.addressName?'#303133':'#999999')+';'}}" bindtap="__e">{{''+(addressData.addressName?addressData.addressName:'请选择所在地区')+''}}</text><text class="gui-list-arrow-right gui-icons"></text></view><view class="row b-b"><text class="tit">详细地址</text><input class="input" type="text" placeholder="详细地址" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','addressDetails','$event',[]],['addressData']]]]]}}" value="{{addressData.addressDetails}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.isDefault==1}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button><gui-area-picker class="vue-ref" vue-id="0e08e190-1" level="{{3}}" cancelText="关闭" data-ref="graceAddressPicker" data-event-opts="{{[['^confirm',[['areaConfirm']]]]}}" bind:confirm="__e" bind:__l="__l"></gui-area-picker></view>