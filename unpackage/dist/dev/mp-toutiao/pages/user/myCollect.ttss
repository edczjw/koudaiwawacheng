page, .content {
  background: #f8f8f8;
}
.content {
  padding-top: 16rpx;
}
.dolls-list {
  display: flex;
  flex-wrap: wrap;
  padding: 0 30rpx;
  background: #fff;
}
.dolls-list .dolls-item {
  display: flex;
  flex-direction: column;
  width: 48%;
  padding-bottom: 40rpx;
}
.dolls-list .dolls-item:nth-child(2n+1) {
  margin-right: 4%;
}
.dolls-list .image-wrapper {
  width: 100%;
  height: 330rpx;
  border-radius: 3px;
  overflow: hidden;
}
.dolls-list .image-wrapper image {
  width: 100%;
  height: 100%;
  opacity: 1;
}
.dolls-list .title {
  font-size: 32rpx;
  color: #303133;
  line-height: 80rpx;
}
.dolls-list .price-box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right: 10rpx;
  font-size: 24rpx;
  color: #909399;
}
.dolls-list .price {
  font-size: 32rpx;
  color: #fa436a;
  line-height: 1;
}

